    "express": "^4.16.4",
    "ktool": "^1.0.51",
    "mkdirp": "^0.5.1",
    "mongodb": "^3.2.2",
    "mysql": "^2.16.0",
    "redis": "^2.8.0",
    "vlog": "^0.2.12",
    "xml2js": "^0.4.19"
  },
