{
  "ver": 1,
  "project": "[#projectName]",
  "h$_key":"[#random]",
  "ts": 0,
  "s$_sessionKey": "[#random]",
  "sessionExpire": 1800,
  "sessionType": "mem",
  "failLoginRedirect": "login",
  "s$_apiKey": "[#apiKey]",
  "fail2ban": 3600,
  "fail2banTimes": 5,
  "firstUser":{
    "isFirst":true,
    "loginName":"admin",
    "loginPwd":"helloFirst",
    "level":10,
    "_id":0
  },
  "redis": {
    "init": false,
    "s$_redisIP": "127.0.0.1",
    "s$_redisPort": 6379,
    "s$_pwd": false
  },
  "mongo": {
    "init": false,
    "s$_mongoDbName": "[#projectName]",
    "s$_mongoUrl": "mongodb://user:pwd@127.0.0.1:27017/[#projectName]"
  },
  "mysql": {
    "init": false,
    "s$_isCluster":false,
    "server": {
      "connectionLimit": 60,
      "s$_host": "127.0.0.1",
      "s$_user": "user",
      "s$_password": "pwd",
      "s$_database": "[#projectName]"
    },
    "servers": {
      "master1": {
        "connectionLimit": 60,
        "s$_host": "127.0.0.1",
        "s$_user": "user",
        "s$_password": "pwd",
        "s$_database": "[#projectName]"
      },
      "slave1": {
        "connectionLimit": 60,
        "s$_host": "127.0.0.1",
        "s$_user": "user",
        "s$_password": "pwd",
        "s$_database": "[#projectName]"
      }
    }
  },
  "startPort": [#port]
}
